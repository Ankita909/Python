
import numpy as np

#--------------ones()------------------

#help(np.ones)
x=np.ones(6) #by-default float
print(x)

y=np.ones((3),dtype="int")
print(y)

# Multi-dimensional
z=np.ones([3,4])  #list
print(z)
z=np.ones((3,4))
print(z) #tuple

# if the order is changed

a=np.ones(4,order="F")
print(a)

b=np.ones((3,4),order="F")
print(b)

#-------------empty()--------------
#help(np.empty)
c=np.empty(5)
print(c)
y=np.empty([3,4],dtype="int")
print(y)
